{
  "autoAccept": [
    "ansible-inventory",
    "ansible-playbook --check",
    "cargo build",
    "cargo test",
    "cat",
    "cd",
    "docker build",
    "docker inspect",
    "docker logs",
    "docker network inspect",
    "docker network ls",
    "docker ps",
    "echo",
    "find",
    "git diff",
    "git fetch",
    "git log",
    "git status",
    "grep",
    "head",
    "less",
    "ls",
    "make",
    "npm list",
    "npm run",
    "npm test",
    "ps",
    "pwd",
    "systemctl status",
    "tail"
  ],
  "autoUpdates": true,
  "permissions": {
    "allow": [
      "Bash",
      "Edit",
      "Glob",
      "Grep",
      "LS",
      "MultiEdit",
      "Read",
      "Task",
      "WebFetch",
      "WebSearch",
      "Write"
    ]
  },
  "theme": "dark",
  "verbose": false
}
