{
  "general": {
    "vimMode": false,
    "disableAutoUpdate": false
  },
  "ide": {
    "enabled": true
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "tools": {
    "core": [
      "run_shell_command(ansible-inventory)",
      "run_shell_command(ansible-playbook --check)",
      "run_shell_command(cargo build)",
      "run_shell_command(cargo test)",
      "run_shell_command(cat)",
      "run_shell_command(cd)",
      "run_shell_command(docker build)",
      "run_shell_command(docker inspect)",
      "run_shell_command(docker logs)",
      "run_shell_command(docker network inspect)",
      "run_shell_command(docker network ls)",
      "run_shell_command(docker ps)",
      "run_shell_command(echo)",
      "run_shell_command(find)",
      "run_shell_command(git diff)",
      "run_shell_command(git fetch)",
      "run_shell_command(git log)",
      "run_shell_command(git status)",
      "run_shell_command(grep)",
      "run_shell_command(head)",
      "run_shell_command(less)",
      "run_shell_command(ls)",
      "run_shell_command(make)",
      "run_shell_command(npm list)",
      "run_shell_command(npm run)",
      "run_shell_command(npm test)",
      "run_shell_command(ps)",
      "run_shell_command(pwd)",
      "run_shell_command(systemctl status)",
      "run_shell_command(systemctl tail)",
      "web_fetch"
    ]
  },
  "ui": {
    "hideBanner": true,
    "hideFooter": true,
    "hideTips": true,
    "showMemoryUsage": false
  }
}
