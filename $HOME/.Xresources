! URxvt.font: xft:DejaVuSansMono:Medium:size=14
! URxvt.font: xft:Inconsolata:Medium:size=14
URxvt.font: xft:SourceCodePro:Medium:size=14

URxvt.iso14755 : false
URxvt.keysym.C-C: eval:selection_to_clipboard
URxvt.keysym.C-V: eval:paste_clipboard
URxvt.perl-ext-common: default,eval
URxvt.saveLines: 65535
URxvt.scrollBar: false
URxvt.shading: 10
URxvt.transparent: false

